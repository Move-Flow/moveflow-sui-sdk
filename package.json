{
  "name": "@moveflow/sui-sdk.js",
  "version": "0.2.6",
  "description": "Typescript SDK for MoveFlow on SUI blockchain",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Move-Flow/moveflow-sui-sdk.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/Move-Flow/moveflow-sui-sdk/issues"
  },
  "homepage": "https://github.com/Move-Flow/moveflow-sui-sdk#readme",
  "keywords": [
    "MoveFlow",
    "SUI",
    "SDK"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "./dist/tsc/index.js",
  "types": "./dist/tsc/index.d.ts",
  "scripts": {
